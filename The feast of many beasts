package kata


func main(){
  
    Feast ("great blue heron", "garlic naan")
}

func Feast(beast string, dish string) bool {
  //animal:= []string {beast}
  //food := []string {dish}
  
  //ultAnim := (len(animal)-1)
  //ultFood := (len(food)-1)
  
  if beast[0] == dish[0] && beast[len(beast)-1] == dish[len(dish)-1]{
  
      return true
  
  }
  
    return false
}
